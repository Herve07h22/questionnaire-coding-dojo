{
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "outDir": "./dist",
    "strict": true,
    "sourceMap": true,
    "noImplicitReturns": true
  },
  "include": ["vite.config.ts"]
}
